<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.Dao.StudentMapper">
	<select id="GetListStudent" resultType="Student" resultMap="result">
		SELECT * FROM Student
	</select>

	<resultMap type="com.example.Model.StudentDto" id="result">
		<id property="idStudent" column="idStudent" />
		<result property="name" column="name" />
		<result property="age" column="age" />
		<result property="classStudent" column="class" />
	</resultMap>
</mapper>
